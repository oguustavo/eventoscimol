{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  }
} 